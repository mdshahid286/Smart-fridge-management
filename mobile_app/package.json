{
  "name": "smart-refrigerator-mobile-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "start:clear": "npx expo start -c",
    "start:tunnel": "npx expo start --tunnel --clear",
    "start:lan": "npx expo start --lan --clear",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "clean": "rm -rf node_modules .expo android/build android/app/build ios/build",
    "clean:win": "powershell -Command \"Remove-Item -Recurse -Force node_modules,.expo,android\\build,android\\app\\build -ErrorAction SilentlyContinue\"",
    "reset": "npm run clean:win && npm install && npx expo start --clear"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "axios": "^1.13.1",
    "expo": "~54.0.0",
    "expo-image-picker": "~17.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.0.2"
  }
}
