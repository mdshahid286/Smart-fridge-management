{
  "expo": {
    "name": "ChillTrack",
    "slug": "chilltrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartrefrigerator.app"
    },
    "android": {
      "package": "com.smartrefrigerator.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "scheme": "chilltrack",
    "extra": {
      "eas": {
        "projectId": "replace-this-with-your-project-id"
      }
    },
  }
}
